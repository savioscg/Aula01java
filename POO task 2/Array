public class Array {
    public static void main(String[] args) {
        int[] numeros = {1, 5, 7, 2, 4, 9, 3, 8, 6, 10};
        for (int i = 0; i < numeros.length; i++) {
            if (numeros[i] > 3) {
                System.out.println("Valor na posição " + i + ": " + numeros[i]);
            }
        }
    }
}